{
	"manifest_version": 2,
    "version": "0.1",
    "name": "Search Site",
    "description": "Search webpages matching site.",
    "homepage_url": "https://github.com/samwhelp/webextension-practice/webextension-search-site",
    "icons": {
        "48": "icons/search.48.png"
    },
	"permissions": [
		"tabs"
	],
	"browser_action": {
		"default_icon": "icons/search.32.png",
		"default_title": "Search Site",
		"default_popup": "search.html"
	}
}
